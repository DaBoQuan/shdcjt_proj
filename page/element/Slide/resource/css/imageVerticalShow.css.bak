@CHARSET "UTF-8";
/* images display area */
.imgArea{
	width:100%;overflow:hidden;
}


/* small images area */
#navigation_zz {

	
	BACKGROUND: url(../image/bg-btn.png) repeat-y 0px 0px;
	TEXT-ALIGN: center; 
	PADDING-BOTTOM: 0px; 
	PADDING-LEFT: 3px; 
	PADDING-RIGHT: 0px; 
	PADDING-TOP: 4px;
	PADDING-BOTTOM: 0px; 
	MARGIN: 0px; 
	PADDING-LEFT: 0px; 
	PADDING-RIGHT: 0px; 
	PADDING-TOP: 0px;
	WIDTH: 202px; 
	position:relative;
	
	height:185px;
	width:201px;
	overflow:hidden;
	float:left;

	}

/* small images li*/
#navigation_zz li {
	POSITION: relative; 
	
	FLOAT: left; 
	HEIGHT: 46px; 
	OVERFLOW: hidden;
	Z-INDEX: 7; 
	width:201px;
	MARGIN-top:0px;
	margin-left:0px;
}

/* small images area a */
#navigation_zz li a {
	PADDING-TOP: 0px;
	font: bold 10px arial;
	text-decoration: none;
	color: #000;
	outline: none;
	HEIGHT: 46px; 
	width:201px;
	margin-bottom : 0.5px;
	OVERFLOW: hidden;
	z-index: 7;
	
	float: left;
	text-align:left;

	overflow: hidden;
}
#navigation_zz li A:link,#navigation li A:visited, #navigation li A:hover,#navigation li A:active {
	border: none;
}

.moveClass {
	z-index:10;
	}


